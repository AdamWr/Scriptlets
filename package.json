{
    "name": "scriptlets",
    "version": "1.0.0",
    "scripts": {
        "build": "rollup -c",
        "watch": "rollup -c -w",
        "corelibs": "babel-node corelibs.build.js",
        "test": "rollup -c --environment UI_TEST && node tests/index.js",
        "gui-test": "rollup -c --environment UI_TEST && open tests/tests.html",
        "lint": "eslint ."
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run lint"
        }
    },
