{
    "name": "scriptlets",
    "version": "1.0.4",
    "scripts": {
        "build": "rollup -c",
        "watch": "rollup -c -w",
        "corelibs": "rollup -c --environment CLEAN && babel-node corelibs.build.js",
        "test": "rollup -c --environment UI_TEST && node tests/index.js",
        "test-watch": "rollup -c -w --environment UI_TEST",
        "gui-test": "rollup -c --environment UI_TEST && open http://127.0.0.1:8585 && node ./tests/server.js",
        "lint": "eslint ."
    },
    "husky": {
        "hooks": {
            "pre-commit": "yarn run lint"
        }
    },
    "author": "devteam@adguard.com",
    "license": "LGPL-3.0",
    "devDependencies": {
        "@babel/cli": "^7.2.3",
        "@babel/core": "^7.2.2",
        "@babel/node": "^7.2.2",
        "@babel/plugin-transform-regenerator": "^7.0.0",
        "@babel/plugin-transform-runtime": "^7.2.0",
        "@babel/preset-env": "^7.2.3",
        "eslint": "^5.14.1",
        "eslint-config-airbnb-base": "^13.1.0",
        "eslint-plugin-import": "^2.16.0",
        "husky": "^1.3.1",
        "node-qunit-puppeteer": "^1.0.11",
        "qunit": "^2.9.1",
        "rollup": "^0.68.2",
        "rollup-plugin-babel": "^4.1.0",
        "rollup-plugin-cleanup": "^3.1.1",
        "rollup-plugin-commonjs": "^9.3.4",
        "rollup-plugin-node-resolve": "^4.0.0",
        "sinon": "^7.3.1",
        "js-yaml": "^3.13.1"
    },
    "dependencies": {
        "@babel/runtime": "^7.3.1"
    },
    "files": [
        "./dist/"
    ]
}
